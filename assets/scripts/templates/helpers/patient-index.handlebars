
<table id="t01">
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Age</th>
    <th>Height</th>
    <th>Weight</th>
    <th>Phone</th>
    <th>Primary Doctor</th>
  </tr>

  {{#each patients as |patient|}}
    <tr>
      <td>{{patient.first_name}}</td>
      <td>{{patient.last_name}}</td>
      <td>{{patient.age}}</td>
      <td>{{patient.height}}</td>
      <td>{{patient.weight}}</td>
      <td>{{patient.phone}}</td>
      <td>{{patient.primary_doctor}}</td>
    </tr>
  {{/each}}

</table>
